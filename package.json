{
  "name": "@typesafedev/prowling-cat-ts",
  "version": "0.0.2",
  "description": "Animation of prowling cat",
  "main": "index.js",
  "bin": "bin/index.js",
  "scripts": {
    "start": "npm run build && npm run global",
    "build": "tsc -p .",
    "global": "npm i -g && prowling-cat-ts",
    "refresh": "npm install",
    "tscov": "tscov --min-coverage 5",
